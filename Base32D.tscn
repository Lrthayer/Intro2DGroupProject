script/source = "extends Area2D

var hovering = false
var locked = false
var clicked = false
var toggled = 0
var lockToggle = 0

var menuToggle = 0

func _ready():
	set_process(true)
	
func _process(delta):
	if hovering:
		#if locked let parent node know
		if locked:
			get_parent().cursorState = \"locked\"
		
		if Input.is_action_pressed(\"middle_click\"):
			if lockToggle < 1:
				if locked:
					locked = false
					print(\"unlocked\")
				else:
					locked = true
					print(\"locked\")
				lockToggle += 1
		else:
			lockToggle = 0
	if (hovering || clicked) && !locked:
		if  Input.is_action_pressed(\"left_click\"):
			if toggled < 1:
				if clicked:
					clicked = false
				else:
					clicked = true
				toggled += 1
		else:
			toggled = 0
			
		if  Input.is_action_pressed(\"edit\") && !locked:
			if menuToggle < 1:
				#if already open, close
				if self.get_node(\"Base3Menu\").visible:
					self.get_node(\"Base3Menu\").visible = false
					#self.get_node(\"TurrentMenu/StatsVBox\").set_hidden(true)
					#self.get_node(\"TurrentMenu/VisualVBox\").set_hidden(true)
					self.get_node(\"Base3Menu\").set_pos(Vector2(30000,30000))
					#set root state to placing
					self.get_node(\"/root/Main\").editorState = \"placing\"
					self.get_node(\"/root/Main/Camera2D/StatesArea/StatesHBox/PlacingButton\")._on_PlacingButton_pressed()
				else:
					self.get_node(\"Base3Menu\").set_pos(Vector2(200,-100))
					self.get_node(\"Base3Menu\").visible = true
					#set root state to editing
					self.get_node(\"/root/Main\").editorState = \"editing\"
					self.get_node(\"/root/Main/Camera2D/StatesArea/StatesHBox/EditingButton\")._on_EditingButton_pressed()
				menuToggle += 1
		else:
			menuToggle = 0
			
			
			
		if clicked:
			self.global_position = get_global_mouse_position()
			print(\"wut\")
			#fixes spawning a new object while moving this object
			if !get_node(\"/root/Main\").overButton:
				get_node(\"/root/Main\").overButton = true
		
		if  Input.is_action_pressed(\"right_click\") && !locked:
			self.get_node(\"/root/Main\").set_hovering(false)
			self.free()

func _on_Base32D_mouse_enter():
	hovering = true


func _on_Base32D_mouse_exit():
	hovering = false
"

