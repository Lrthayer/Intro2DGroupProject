#called every frame
func _process(delta):
	var mosLoc = get_global_mouse_pos()
	var posOffset = Vector2(mosLoc.x-20, mosLoc.y-20);
	#get_child(1).set_global_pos(posOffset)
	if  Input.is_action_pressed(\"left_click\"):
