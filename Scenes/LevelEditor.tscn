script/source = "extends Area2D

var hovering = false
var locked = false
var clicked = false
var toggled = 0
var lockToggle = 0

func _ready():
	set_process(true)
	
func _process(delta):
	if hovering:
		if Input.is_action_pressed(\"middle_click\"):
			if lockToggle < 1:
				if locked:
					locked = false
					print(\"unlocked\")
				else:
					locked = true
					print(\"locked\")
				lockToggle += 1
		else:
			lockToggle = 0
	if (hovering || clicked) && !locked:
		if  Input.is_action_pressed(\"left_click\"):
			if toggled < 1:
				if clicked:
					clicked = false
					print(\"false\")
				else:
					clicked = true
					print(\"true\")
				toggled += 1
		else:
			toggled = 0
			
		if clicked:
			self.set_global_pos(get_global_mouse_pos())
			#fixes spawning a new object while moving this object
			if !get_parent().overButton:
				get_parent().overButton = true
		
		if  Input.is_action_pressed(\"right_click\") && !locked:
			self.get_parent().set_hovering(false)
			self.free()

func _on_Base12D_mouse_enter():
	hovering = true

func _on_Base12D_mouse_exit():
	hovering = false
"

