var currentObject
var objectIndex = 0
var overButton = false
var pressed = false

#starts when loaded up in scene
func _ready():
	set_process(true)
	turrent = self.get_node(\"TurretArea\")
	stun = self.get_node(\"MoverArea\")
	base = self.get_node(\"Base12D\")
	base2 = self.get_node(\"Base22D\")
